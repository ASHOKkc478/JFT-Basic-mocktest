        <button type="button" onclick="calculateScore()">Submit Answers</button>
    </form>

    <div class="result" id="result"></div>

    <script>
        // Correct answers for all 100 questions
        const correctAnswers = [
            "a", "a", "c", "c", "d", "c", "d", "d", "b", "b", // 1-10
            "b", "d", "a", "d", "c", "b", "a", "d", "a", "c", // 11-20
            "a", "d", "a", "c", "a", "b", "c", "b", "b", "a", // 21-30
            "b", "b", "c", "b", "a", "b", "c", "d", "c", "d", // 31-40
            "b", "d", "d", "a", "c", "d", "d", "c", "d", "b", // 41-50
            "d", "d", "b", "a", "c", "b", "c", "b", "a", "a", // 51-60
            "d", "b", "b", "d", "d", "c", "c", "a", "c", "b", // 61-70
            "b", "a", "b", "c", "b", "c", "b", "b", "c", "d", // 71-80
            "b", "d", "b", "d", "b", "d", "c", "d", "d", "b", // 81-90
            "a", "b", "d", "a", "c", "c", "d", "c", "c", "b"  // 91-100
        ];

        function calculateScore() {
            let score = 0;
            for (let i = 1; i <= 100; i++) {
                const selectedAnswer = document.querySelector(`input[name="q${i}"]:checked`);
                if (selectedAnswer && selectedAnswer.value === correctAnswers[i - 1]) {
                    score++;
                }
            }
            document.getElementById("result").innerText = `Your score is ${score} out of 50.`;
        }
    </script>
</body>
</html>

